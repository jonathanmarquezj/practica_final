				+breadcrumb(bread_form_elements)

			.l-spaced
				//Basic Elements
				.l-box.l-spaced-bottom
					.l-box-header
						h2.l-box-title <span>Basic</span> Elements
						+box-options

					.l-box-body.l-spaced
						form.form-horizontal(role='form')


							.form-group
								label.col-sm-3.control-label(for='basicText') Text
								.col-sm-9
									input#basicText.form-control(type='text', placeholder='Text')

							.form-group
								label.col-sm-3.control-label(for='basicEmail') Email
								.col-sm-9
									input#basicEmail.form-control(type='email', placeholder='Email')

							.form-group
								label.col-sm-3.control-label(for='basicPassword') Password
								.col-sm-9
									input#basicPassword.form-control(type='password', placeholder='Password')

							.form-group
								label.col-sm-3.control-label(for='disabledInput') Disabled Text
