//- ------------------------------------
//- BASE LAYOUT
//- ------------------------------------
extends aaa-master-layout


//- ------------------------------------
//- PAGE CONFIGURATION
//- ------------------------------------
prepend config
	- var link = "form-elements.html"
	-var page = "form.fo-elements"
	-var pageSubTitle = " - Form - Elements"
	-var styles = [ 'select2' ]
	-var scripts = [ 'form.maskedInput', 'form.autogrow', 'form.textareaCounter', 'form.bootstrapTouchspin', 'form.switchery', 'form.onoff', 'form.fancySelect', 'form.select2', 'form.checkradios', 'form.checkBo', 'x.call.form.elements' ]


//- ------------------------------------
//- PAGE CONTENT
//- ------------------------------------
block content

	//SECTION
	section.l-main-container


		include components/layout/sidebar/sidebar_1/sidebar_1__t_1__left

		//Main Content
		section.l-container

			include components/layout/header/header_1/header_1__t_1

			.l-page-header
